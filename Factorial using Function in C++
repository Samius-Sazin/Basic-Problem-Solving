#include<iostream>
using namespace std;

void factorial(int f);

int main()
{
    int n1,n2;
    cin>>n1>>n2;

    factorial(n1);
    factorial(n2);

    return 0;
}

void factorial(int f)
{
    int fact=1;
    for(int i=f; i>=2; i--)
    {
        fact*=i;
    }
    cout<<f<<"! = "<<fact<<endl;
}

/*

n1=4
n2=5
4! = 24
5! = 120

*/
