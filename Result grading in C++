#include<iostream>
using namespace std;
int main()
{
    int mark,i;

    cout<<"Please enter your mark : ";
    cin>>mark;
    if(mark<=100 && mark>=80) cout<<"Grade A+"<<endl;
    else if(mark<80 && mark>=75) cout<<"Grade A"<<endl;
    else if(mark<75 && mark>=70) cout<<"Grade A-"<<endl;
    else if(mark<70 && mark>=65) cout<<"Grade B+"<<endl;
    else if(mark<65 && mark>=60) cout<<"Grade B"<<endl;
    else if(mark<60 && mark>=55) cout<<"Grade B-"<<endl;
    else if(mark<55 && mark>=50) cout<<"Grade C"<<endl;
    else if(mark<50 && mark>=40) cout<<"Grade D"<<endl;
    else cout<<"You are FAIL. Need improvement"<<endl;
    
    cout<<"This is your result"<<endl;

    return 0;
}

